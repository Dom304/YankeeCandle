<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootpane" prefHeight="600" prefWidth="600" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="customer.ViewCandlesController">
   <children>
      <Button fx:id="exitProgram" layoutX="1145.0" layoutY="47.0" mnemonicParsing="false" onAction="#exitProgram" prefHeight="44.0" prefWidth="113.0" text="Sign out" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="14.0">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <ImageView fitHeight="197.0" fitWidth="529.0" layoutX="258.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" AnchorPane.leftAnchor="258.0" AnchorPane.rightAnchor="480.875" AnchorPane.topAnchor="14.0">
         <image>
            <Image url="@../main/ykcvlogo.png" />
         </image>
      </ImageView>
      <Button fx:id="viewCartButton" layoutX="1008.0" layoutY="14.0" mnemonicParsing="false" onAction="#viewCartButton" prefHeight="44.0" prefWidth="113.0" text="View Cart">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="home" layoutX="886.0" layoutY="14.0" mnemonicParsing="false" onAction="#home" prefHeight="44.0" prefWidth="113.0" text="Home">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <TableView fx:id="product_table" layoutX="-1.0" layoutY="227.0" prefHeight="709.0" prefWidth="1256.0">
        <columns>
          <TableColumn fx:id="product_id" prefWidth="77.0" text="ID" />
          <TableColumn fx:id="product_name" prefWidth="301.0" text="Name" />
            <TableColumn fx:id="product_description" minWidth="0.0" prefWidth="384.0" text="Description" />
            <TableColumn fx:id="product_price" prefWidth="47.0" text="Price" />
            <TableColumn fx:id="product_quantity" prefWidth="74.0" text="Quantity" />
            <TableColumn fx:id="product_image" prefWidth="182.0" text="Image" />
            <TableColumn fx:id="product_categoryid" minWidth="0.0" prefWidth="102.0" text="Category ID" />
            <TableColumn fx:id="product_addToCart" minWidth="0.0" prefWidth="88.0" text="Add to Cart" />
        </columns>
      </TableView>
      <Button fx:id="testbtn" layoutX="123.0" layoutY="100.0" mnemonicParsing="false" onAction="#refreshBtn" text="TestMe" />
   </children>
</AnchorPane>
