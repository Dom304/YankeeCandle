<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="290.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="admin.EditUserController">
   <children>
      <Label fx:id="dialog_title" layoutX="86.0" layoutY="14.0" text="Add User">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <TextField fx:id="user_name" layoutX="46.0" layoutY="59.0" promptText="Name">
         <font>
            <Font size="16.0" />
         </font>
      </TextField>
      <TextField fx:id="user_email" layoutX="46.0" layoutY="104.0" promptText="Email">
         <font>
            <Font size="16.0" />
         </font>
      </TextField>
      <TextField fx:id="user_password" layoutX="46.0" layoutY="151.0" promptText="Password">
         <font>
            <Font size="16.0" />
         </font>
      </TextField>
      <ComboBox fx:id="role_select" layoutX="46.0" layoutY="200.0" prefHeight="35.0" prefWidth="198.0" promptText="Role" />
      <Button layoutX="46.0" layoutY="335.0" mnemonicParsing="false" onAction="#save" text="Save">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button layoutX="183.0" layoutY="335.0" mnemonicParsing="false" onAction="#resetButton" text="Reset">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <CheckBox fx:id="user_active" layoutX="183.0" layoutY="290.0" mnemonicParsing="false" prefHeight="17.0" prefWidth="61.0" selected="true" text="Active" />
      <TextField fx:id="user_question" layoutX="46.0" layoutY="246.0" promptText="Security Question">
         <font>
            <Font size="16.0" />
         </font>
      </TextField>
   </children>
</AnchorPane>
